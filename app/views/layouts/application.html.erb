<!DOCTYPE html>
<html>
<head>
  <title>Testapp 0.1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= link_to image_tag('logo.gif', width: 75), root_path %>
<%= link_to 'Регистрация', registration_path %>
<%= link_to 'Вход', sessions_login_path %>
<%= link_to 'Выйти', sessions_logout_path %>

<%= current_user.name if current_user %>

<%= yield %>

</body>
</html>
